include ".Xcolors"

Xft.hinting:            false
Xft.hintstyle:          hintnone
Xft.antialias:          true
Xft.rgba:               rgba
Xft.dpi:                96

!urxvt settings
urxvt*depth:            32
urxvt*geometry:         56x38
urxvt*saveLines:        65535
urxvt*scrollTtyKeypress:true
urxvt*scrollTtyOutput:  false
urxvt*scrollBar:        false
urxvt*loginShell:       true
urxvt*shading:          55
urxvt*tintColor:        #202222
urxvt*font:             xft:DejaVu Sans Mono:normal:size=9
!urxvt*print-pipe:       cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
urxvt*secondaryScroll:  true
urxvt*background:       rgba:0000/0000/0000/dddd
urxvt*cursorBlink:      true
urxvt*cursorUnderline:  true
urxvt*perl-ext-common:  default,clipboard
urxvt*foreground:       rgba:ffff/ffff/ffff/dddd
urxvt*urlLauncher:      /usr/bin/firefox
urxvt*matcher.button:   1
urxvt*xftAntialias:     true
urxvt*borderless:       true
urxvt*internalBorder:   5
urxvt*externalBorder:   5
URxvt*imLocale:         de_DE.UTF-8
urxvt*perl-ext-common:  default,matcher,clipboard
!urxvt.perl-ext-common:  default,tabbed

!---Colors----
!black
*color0:#050505
*color8:#a9a9a9
!red
*color1:#CC0000
*color9:#EF2929
!green
*color2:#3B8526
*color10:#58A642
!yellow
*color3:#e0a524
*color11:#edB85c
!blue
*color4:#1D576D
*color12:#31788E
!magenta
*color5:#500000
*color13:#590000
!cyan
*color6:#8edcff
*color14:#87adfc
!white
*color7:#dedede
*color15:#f2f2f2


